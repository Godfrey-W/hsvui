{
  "name": "hsvui",
  "version": "0.0.1",
  "description": "Vue basic components",
  "main": "lib/hsvui.js",
  "style": "lib/styles/hsvui.css",
  "files": [
    "lib",
    "packages"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "webpack-serve --config build/webpack.dev.config.js",
