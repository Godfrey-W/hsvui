{
  "name": "hsvui",
  "version": "0.0.6",
  "description": "A Component Library for Vue.js.",
  "main": "lib/hsvui.common.js",
  "files": [
    "lib",
    "src",
    "packages"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:file": "node build/bin/gen-cssfile.js && node build/bin/build-entry.js",
    "dev": "npm run build:file && cross-env NODE_ENV=development webpack-serve --config build/webpack.demo.js",
    "build:theme": "gulp build --gulpfile packages/hsvui-css/gulpfile.js && cp-cli packages/hsvui-css/lib lib/hsvui-css",
    "build:demo": "npm run build:file && cross-env NODE_ENV=production webpack --config build/webpack.demo.js",
    "build:common": "webpack --config build/webpack.common.js",
    "build:component": "webpack --config build/webpack.component.js",
    "build:dist": "webpack --config build/webpack.conf.js",
    "clean": "rimraf lib && rimraf packages/*/lib",
    "dist": "npm run clean && npm run build:file && npm run build:dist && npm run build:common && npm run build:component && npm run build:theme"
  },
  "keywords": [
    "hsvui",
    "vue",
    "vue.js",
    "component",
    "components",
    "ui"
  ],
  "homepage": "https://godfrey-w.github.io/hsvui/",
  "repository": {
    "type": "git",
    "url": "git@github.com:Godfrey-W/hsvui.git"
  },
  "author": "godfrey",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Godfrey-W/hsvui/issues"
  },
  "unpkg": "lib/index.js",
  "style": "lib/hsvui-css/index.css",
  "peerDependencies": {
    "vue": ">= 2.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "autoprefixer": "^9.4.3",
    "babel-loader": "^8.0.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "cp-cli": "^1.1.2",
    "cross-env": "^5.2.0",
    "css-loader": "^2.0.2",
    "file-loader": "^3.0.1",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-cssmin": "^0.2.0",
    "gulp-postcss": "^8.0.0",
    "gulp-sass": "^4.0.2",
    "html-webpack-plugin": "^3.2.0",
    "json-templater": "^1.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "os": "^0.1.1",
    "postcss-loader": "^3.0.0",
    "progress-bar-webpack-plugin": "^1.11.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.0",
    "uppercamelcase": "^3.0.0",
    "url-loader": "^1.1.2",
    "vue": "^2.5.21",
    "vue-loader": "^15.4.2",
    "vue-router": "^3.0.2",
    "vue-template-compiler": "^2.5.21",
    "webpack": "^4.28.2",
    "webpack-cli": "^3.1.2",
    "webpack-serve": "^2.0.3"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
