    "build": "npm run build:style && cross-env NODE_ENV=production webpack --color --config build/webpack.build.config.js",
    "build:demo": "cross-env NODE_ENV=production webpack --color --config build/webpack.demo.config.js"
  },
  "keywords": [
    "hsvui",
    "vue",
    "vue.js",
    "component",
    "components",
    "ui"
  ],
  "author": "godfrey",
  "license": "ISC",
  "peerDependencies": {
    "vue": ">= 2.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/runtime": "^7.2.0",
    "autoprefixer": "^9.4.3",
    "babel-loader": "^8.0.4",
    "cross-env": "^5.2.0",
    "css-loader": "^2.0.1",
    "gulp": "^4.0.0",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-clean-css": "^4.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.11.0",
    "postcss-loader": "^3.0.0",
    "progress-bar-webpack-plugin": "^1.11.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "vue": "^2.5.21",
    "vue-loader": "^15.4.2",
    "vue-router": "^3.0.2",
    "vue-template-compiler": "^2.5.21",
    "webpack": "^4.28.0",
    "webpack-cli": "^3.1.2",
    "webpack-merge": "^4.1.5",
    "webpack-serve": "^2.0.3"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
